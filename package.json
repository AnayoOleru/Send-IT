{
  "name": "send-it",
  "version": "1.0.0",
  "description": "SendIT is a courier service that helps users deliver parcels to different destinations",
  "main": "app.js",
  "scripts": {
    "db": "node ./src/databaseConnection/db.js createAllTables",
    "db-drop": "node ./src/databaseConnection/db.js dropAllTables && node ./server/db/tables.js dropUserTable",
    "refresh-db": "npm run db-drop && npm run db",
    "start": "babel-node app",
    "test:server": "mocha --compilers js:@babel/register ./test/parcelRouterTest.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AnayoOleru/Send-IT.git"
  },
  "author": "Anayo Oleru",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AnayoOleru/Send-IT/issues"
  },
  "homepage": "https://github.com/AnayoOleru/Send-IT#readme",
  "dependencies": {
    "@babel/core": "^7.1.5",
    "@babel/polyfill": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "body-parser": "^1.18.3",
    "chai-http": "^4.2.0",
    "crypto": "^1.0.1",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "moment": "^2.22.2",
    "pg": "^7.6.1",
    "uuidv4": "^2.0.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "eslint": "^5.8.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "mocha": "^5.2.0"
  }
}
